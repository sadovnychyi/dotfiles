{
    "editor.fontFamily": "PragmataPro Mono Liga",
    "workbench.colorTheme": "Chromodynamics",
    "telemetry.enableTelemetry": false,
    "gitlens.advanced.messages": {
        "suppressLineUncommittedWarning": true,
        "suppressShowKeyBindingsNotice": true
    },
    "workbench.colorCustomizations": {
        "sideBar.background": "#131313",
        "input.background": "#131313",
        "activityBar.background": "#131313",
        "activityBar.border": "#000000",
        "editor.background": "#000000",
        "statusBar.background": "#131313",
        "statusBar.foreground": "#a8a8a8",
        "editorRuler.foreground": "#ffffff1f",
        "editorGroupHeader.tabsBackground": "#131313",
        "tab.border": "#131313",
        "tab.inactiveBackground": "#131313",
        "editorBracketMatch.border": "#ffffff00",
        "editorBracketMatch.background": "#f0f0f352",
        "editor.selectionBackground": "#888",
        "editor.selectionHighlightBackground": "#555",
        "editor.selectionHighlightBorder": "#ffffff00"
    },
    "editor.tokenColorCustomizations": {
        "[Chromodynamics]": {
            "comments": "#747475"
        }
    },
    "editor.minimap.enabled": false,
    "editor.renderIndentGuides": false,
    "editor.renderLineHighlight": "all",
    "editor.renderWhitespace": "boundary",
    "editor.roundedSelection": false,
    "editor.tabSize": 2,
    "gitlens.advanced.telemetry.enabled": false,
    "gitlens.currentLine.enabled": false,
    "editor.links": false,
    "editor.wordWrap": "on",
    "gitlens.codeLens.enabled": false,
    "search.location": "panel",
    "editor.rulers": [
        80
    ],
    "files.trimTrailingWhitespace": true,
    "files.trimFinalNewlines": true,
    "files.insertFinalNewline": true,
    "scm.diffDecorations": "gutter",
    "editor.multiCursorModifier": "ctrlCmd",
    "javascript.preferences.importModuleSpecifier": "non-relative",
    "python.autoComplete.extraPaths": [
        "/usr/local/google_appengine",
        "/usr/local/google_appengine/lib/webapp2-2.5.2",
        "${workspaceFolder}/lib"
    ],
    "python.testing.pytestEnabled": true,
    "atomKeymap.promptV3Features": true,
    "editor.formatOnPaste": true,
    "workbench.iconTheme": "file-icons",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "terminal.explorerKind": "external",
    "debug.internalConsoleOptions": "neverOpen",
    "window.zoomLevel": 0,
    "diffEditor.ignoreTrimWhitespace": true,
    "terminal.integrated.fontSize": 14,
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.drawBoldTextInBrightColors": false,
    "diffEditor.renderSideBySide": false,
    "python.workspaceSymbols.enabled": false,
    "python.jediEnabled": false,
    "workbench.editor.closeEmptyGroups": false,
    "workbench.editor.focusRecentEditorAfterClose": false,
    "workbench.settings.enableNaturalLanguageSearch": false,
    "window.restoreFullscreen": true,
    "window.newWindowDimensions": "fullscreen",
    "explorer.decorations.badges": false,
    "outline.problems.colors": false,
    "search.showLineNumbers": true,
    "search.useGlobalIgnoreFiles": true,
    "editor.fontSize": 14,
    "git.confirmSync": false,
    "git.autofetch": true,
    "breadcrumbs.enabled": false,
    "workbench.sideBar.location": "left",
    "workbench.enableExperiments": false,
    "telemetry.enableCrashReporter": false,
    "javascript.validate.enable": false,
    "python.linting.flake8Enabled": true,
    "go.autocompleteUnimportedPackages": true,
    "go.formatTool": "gofmt",
    "editor.fontLigatures": true,
    "go.lintTool": "golangci-lint",
    "go.lintFlags": [
        "--fast"
    ],
    "editor.codeLens": false,
    "python.dataScience.addGotoCodeLenses": false,
    "python.dataScience.enableCellCodeLens": false,
    "search.enableSearchEditorPreview": true,
    "vscode-home-assistant.hostUrl": "http://home.local:8123",
    "vscode-home-assistant.ignoreCertificates": true,
    "go.useLanguageServer": true,
    "workbench.activityBar.visible": true,
    "go.buildOnSave": "off",
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true,
    },
    "git.suggestSmartCommit": false,
    "scm.diffDecorationsGutterWidth": 2,
    "python.testing.autoTestDiscoverOnSaveEnabled": false,
    "python.analysis.memory.keepLibraryLocalVariables": true,
    "python.analysis.memory.keepLibraryAst": true,
    "python.analysis.openFilesOnly": false,
    "python.experiments.enabled": false,
    "python.formatting.provider": "black",
    "python.testing.promptToConfigure": false,
    "python.venvPath": "~/.local/share/virtualenvs",
    "python.pythonPath": "/usr/local/opt/python/libexec/bin/python",
    "python.dataScience.sendSelectionToInteractiveWindow": false,
    "workbench.editor.enablePreview": false,
    "python.linting.flake8Path": "/usr/local/bin/flake8",
    "python.formatting.blackArgs": [
        "--line-length=80",
        "--skip-string-normalization"
    ],
    "python.formatting.blackPath": "/usr/local/bin/black",
    "eslint.format.enable": true,
    "[javascript]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "editor.formatOnSave": true,
    "[python]": {
        "editor.formatOnPaste": false
    },
    "editor.formatOnSaveTimeout": 1000
}
