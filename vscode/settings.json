{
    "editor.fontFamily": "\"PragmataPro Liga\", Monaco, Menlo, Consolas, \"Droid Sans Mono\", \"Inconsolata\", \"Courier New\"",
    "telemetry.telemetryLevel": "off",
    "workbench.colorCustomizations": {
        "sideBar.background": "#131313",
        "input.background": "#131313",
        "activityBar.background": "#131313",
        "activityBar.border": "#000000",
        "editor.background": "#000000",
        "statusBar.background": "#131313",
        "statusBar.foreground": "#a8a8a8",
        "editorRuler.foreground": "#ffffff1f",
        "editorGroupHeader.tabsBackground": "#131313",
        "tab.border": "#000000",
        "tab.inactiveBackground": "#131313",
        "editorBracketMatch.border": "#ffffff00",
        "editorBracketMatch.background": "#f0f0f352",
        "editor.selectionBackground": "#666",
        "editor.selectionHighlightBackground": "#33333300",
        "editor.selectionHighlightBorder": "#ffffffa2",
        "statusBar.debuggingBackground": "#612400",
        "editorGutter.commentRangeForeground": "#c5c5c540",
    },
    "editor.tokenColorCustomizations": {
        "[Chromodynamics]": {
            "comments": "#747475"
        }
    },
    "editor.renderLineHighlight": "all",
    "editor.renderWhitespace": "boundary",
    "editor.tabSize": 2,
    "editor.links": false,
    "editor.wordWrap": "on",
    "editor.rulers": [
        80
    ],
    "files.trimTrailingWhitespace": true,
    "files.trimFinalNewlines": true,
    "files.insertFinalNewline": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "javascript.preferences.importModuleSpecifier": "non-relative",
    "python.autoComplete.extraPaths": [],
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "terminal.explorerKind": "external",
    "debug.internalConsoleOptions": "neverOpen",
    "diffEditor.ignoreTrimWhitespace": false,
    "terminal.integrated.fontSize": 14,
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.drawBoldTextInBrightColors": false,
    "diffEditor.renderSideBySide": false,
    "workbench.editor.closeEmptyGroups": false,
    "workbench.editor.focusRecentEditorAfterClose": false,
    "workbench.settings.enableNaturalLanguageSearch": false,
    "window.restoreFullscreen": true,
    "window.newWindowDimensions": "fullscreen",
    "outline.problems.colors": false,
    "search.showLineNumbers": true,
    "search.useGlobalIgnoreFiles": true,
    "editor.fontSize": 14,
    "git.confirmSync": false,
    "git.autofetch": true,
    "workbench.enableExperiments": false,
    "javascript.validate.enable": false,
    "python.linting.flake8Enabled": true,
    "go.useLanguageServer": true,
    "[go]": {
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": true,
        },
    },
    "[go.mod]": {
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": true,
        },
    },
    "go.buildOnSave": "off",
    "go.autocompleteUnimportedPackages": true,
    "go.formatTool": "gofmt",
    "editor.fontLigatures": "'git tree'",
    "go.lintTool": "golangci-lint",
    "go.lintFlags": [
        "--fast"
    ],
    "editor.codeLens": false,
    "vscode-home-assistant.hostUrl": "http://home.local:8123",
    "vscode-home-assistant.ignoreCertificates": true,
    "git.suggestSmartCommit": false,
    "scm.diffDecorationsGutterWidth": 2,
    "python.testing.autoTestDiscoverOnSaveEnabled": false,
    "python.experiments.enabled": false,
    "python.formatting.provider": "black",
    "python.testing.promptToConfigure": false,
    "python.venvPath": "~/.local/share/virtualenvs",
    "python.pythonPath": "/opt/homebrew/bin/python3",
    "workbench.editor.enablePreview": true,
    "python.linting.flake8Path": "/opt/homebrew/bin/flake8",
    "python.formatting.blackArgs": [
        "--line-length=80",
        "--skip-string-normalization"
    ],
    "python.formatting.blackPath": "/opt/homebrew/bin/black",
    "eslint.format.enable": true,
    "[javascript]": {
        "editor.codeActionsOnSave": {
            "source.fixAll.eslint": true
        },
        "editor.defaultFormatter": "dbaeumer.vscode-eslint",
        "editor.formatOnSave": true
    },
    "[python]": {
        "editor.formatOnPaste": false,
        "editor.formatOnSave": true,
        "editor.insertSpaces": true,
        "editor.tabSize": 4
    },
    "[yaml]": {
        "editor.formatOnSave": false,
    },
    "extensions.ignoreRecommendations": true,
    "files.defaultLanguage": "${activeEditorLanguage}",
    "editor.linkedEditing": true,
    "editor.semanticHighlighting.enabled": true,
    "search.smartCase": true,
    "workbench.activityBar.visible": false,
    "editor.minimap.enabled": false,
    "activitusbar.activeColour": "#fff6f6",
    "cloudcode.autoDependencies": "off",
    "editor.columnSelection": false,
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.roundedSelection": false,
    "editor.suggest.shareSuggestSelections": true,
    "editor.cursorStyle": "line-thin",
    "activitusbar.views": [
        {
            "name": "explorer",
            "codicon": "files"
        },
        {
            "name": "search",
            "codicon": "search"
        },
        {
            "name": "scm",
            "codicon": "source-control"
        },
        {
            "name": "debug",
            "codicon": "debug"
        },
        {
            "name": "extensions",
            "codicon": "extensions"
        }
    ],
    "activitusbar.inactiveColour": "#a8a8a8",
    "liveshare.allowInvites": "all",
    "currentFilePath.defaultPathStartsFrom": "workSpace",
    "editor.snippetSuggestions": "bottom",
    "workbench.startupEditor": "newUntitledFile",
    "git.autoStash": true,
    "scm.inputFontFamily": "editor",
    "[javascriptreact]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "python.analysis.diagnosticSeverityOverrides": {
        "reportUnboundVariable": "none",
        "reportMissingImports": "none"
    },
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "editor.formatOnSave": true,
    "git.enableCommitSigning": true,
    "editor.find.cursorMoveOnType": false,
    "debug.console.fontFamily": "\"PragmataPro Liga\", Monaco, Menlo, Consolas, \"Droid Sans Mono\", \"Inconsolata\", \"Courier New\"",
    "codespaces.enableSubmoduleCloning": true,
    "jupyter.addGotoCodeLenses": false,
    "jupyter.enableCellCodeLens": false,
    "jupyter.sendSelectionToInteractiveWindow": false,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "stylelint.config": {
        "syntax": "scss"
    },
    "go.toolsManagement.autoUpdate": true,
    "breadcrumbs.enabled": false,
    "workbench.settings.useSplitJSON": true,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.decorations.badges": false,
    "explorer.decorations.badges": false,
    "git.pruneOnFetch": true,
    "python.globalModuleInstallation": true,
    "yaml.format.bracketSpacing": false,
    "yaml.format.singleQuote": true,
    "cloudcode.yaml.format.bracketSpacing": false,
    "cloudcode.yaml.format.singleQuote": true,
    "yaml.format.enable": false,
    "redhat.telemetry.enabled": false,
    "editor.inlineSuggest.enabled": true,
    "github.copilot.enable": {
        "*": true,
        "yaml": true,
        "plaintext": true,
        "markdown": true
    },
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "editor.bracketPairColorization.enabled": true,
    "javascript.inlayHints.enumMemberValues.enabled": true,
    "javascript.inlayHints.parameterNames.enabled": "literals",
    "typescript.inlayHints.enumMemberValues.enabled": true,
    "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
    "typescript.inlayHints.parameterNames.enabled": "literals",
    "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
    "typescript.inlayHints.variableTypes.enabled": true,
    "typescript.inlayHints.parameterTypes.enabled": true,
    "workbench.colorTheme": "Chromodynamics",
    "git.allowNoVerifyCommit": true,
    "git.enableStatusBarSync": false,
    "git.fetchOnPull": true,
    "python.analysis.extraPaths": [],
    "security.workspace.trust.untrustedFiles": "open"
}
